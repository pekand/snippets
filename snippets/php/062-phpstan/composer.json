{
    "name": "app",
    "description": "app",
    "keywords": ["app"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.7",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
