{
    "name": "vendor/package",
    "description": "Example of package",
    "keywords": ["package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Author",
            "email": "author@email.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Package\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\Package\\Providers\\PackageServiceProvider"
            ],
            "aliases": {
                "PackageFacade": "Vendor\\Package\\Facades\\PackageFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
